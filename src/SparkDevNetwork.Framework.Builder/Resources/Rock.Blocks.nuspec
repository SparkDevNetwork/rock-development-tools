  <metadata>
    <id>RockRMS.Rock.Blocks</id>
    <version>{{ RockVersion }}</version>
    <authors>Spark Development Network</authors>
    <license type="file">LICENSE.md</license>
    <icon>Icon.png</icon>
    <projectUrl>https://www.rockrms.com/</projectUrl>
