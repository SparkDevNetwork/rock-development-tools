  </metadata>
  <files>
    <file src="LICENSE.md" target="" />
    <file src="Icon.png" target="" />
    <file src="Rock\Rock.Blocks\{{ BinPath }}\Rock.Blocks.dll" target="lib\net472" />
  </files>
