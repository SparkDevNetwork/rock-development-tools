<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
    <id>RockRMS.Rock</id>
    <version>{{ rockVersion }}</version>
    <authors>Spark Development Network</authors>
    <license type="file">LICENSE.md</license>
    <icon>Icon.png</icon>
    <projectUrl>https://www.rockrms.com/</projectUrl>
    <description>The core Rock assembly for plugin development.</description>
    <repository type="git" url="https://github.com/SparkDevNetwork/Rock" />
    <dependencies>
      <dependency id="EntityFramework" version="6.4.4" exclude="runtime" />
      <dependency id="RockRMS.Rock.Common" version="{{ rockVersion }}" exclude="runtime" />
      <dependency id="RockRMS.Rock.Enums" version="{{ rockVersion }}" exclude="runtime" />
      <dependency id="RockRMS.Rock.Lava.Shared" version="{{ rockVersion }}" exclude="runtime" />
      <dependency id="RockRMS.Rock.ViewModels" version="{{ rockVersion }}" exclude="runtime" />
    </dependencies>
  </metadata>
  <files>
    <file src="LICENSE.md" target="" />
    <file src="Icon.png" target="" />
    <file src="RockRMS.Rock.targets" target="build\" />
    <file src="Rock\Rock\bin\Release\Rock.dll" target="build\net472" />
    <file src="Rock\Rock\bin\Release\Rock.xml" target="build\net472" />
  </files>
</package>